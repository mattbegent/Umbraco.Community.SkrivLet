@model Umbraco.Community.SkrivLet.SkrivLetBlock<Umbraco.Community.SkrivLet.Converters.CheckListBlockData>
<ul class="sl-list">
	@foreach (var item in Model.Data.Items)
	{
		<li class="sl-list__item sl-list__item--check-item">
			<span class="sl-check">
				@if (@item.Checked) {
					<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24" class="sl-check__icon"><path stroke="currentColor" stroke-linecap="round" stroke-width="2" d="M7 12L10.4884 15.8372C10.5677 15.9245 10.705 15.9245 10.7844 15.8372L17 9"></path></svg>
				}
			</span>
			<span class="sl-check__text @(item.Checked ? "sl-check__text--checked" : "")">
				@Html.Raw(item.Text)
			</span>

		</li>
	}
</ul>
