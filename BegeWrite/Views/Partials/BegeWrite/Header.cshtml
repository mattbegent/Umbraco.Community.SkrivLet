@model BegeWrite.BegeWriteBlock<BegeWrite.Converters.HeaderBlockData>
@switch (Model.Data.Level)
{
	case 1:
		<h1>@Html.Raw(Model.Data.Text)</h1>
		break;
	case 2:
		<h2>@Html.Raw(Model.Data.Text)</h2>
		break;
	case 3:
		<h3>@Html.Raw(Model.Data.Text)</h3>
		break;
	case 4:
		<h4>@Html.Raw(Model.Data.Text)</h4>
		break;
	case 5:
		<h5>@Html.Raw(Model.Data.Text)</h5>
		break;
	case 6:
		<h6>@Html.Raw(Model.Data.Text)</h6>
		break;
}
